{
    "debug": true,
    "host": "0.0.0.0",
    "port": 6001,
    "websockets": {
        "activityTimeout": 600000,
        "pongTimeout": 120000,
        "pingInterval": 60000
    },
    "appManager.driver": "array",
    "appManager.array.apps": [
        {
            "id": "app-id",
            "key": "app-key",
            "secret": "app-secret",
            "enableClientMessages": true,
            "enableStatistics": true,
            "maxConnections": 10000,
            "maxBackendEventsPerSecond": 10000,
            "maxClientEventsPerSecond": 10000,
            "maxReadRequestsPerSecond": 10000,
            "maxEventPayloadInKb": 10240,
            "webhooks": [
                {
                    "url": "http://localhost:8000/api/pusher/webhook",
                    "event_types": [
                        "member_added",
                        "member_removed",
                        "channel_occupied",
                        "channel_vacated"
                    ]
                }
            ]
        }
    ]
}