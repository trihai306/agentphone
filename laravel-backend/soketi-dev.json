{
    "debug": true,
    "host": "0.0.0.0",
    "port": 6001,
    "websockets.activityTimeout": 300000,
    "websockets.pongTimeout": 60000,
    "websockets.pingInterval": 50000,
    "appManager.driver": "array",
    "appManager.array.apps": [
        {
            "id": "app-id",
            "key": "app-key",
            "secret": "app-secret",
            "enableClientMessages": true,
            "enableStatistics": true,
            "maxConnections": -1,
            "maxBackendEventsPerSecond": -1,
            "maxClientEventsPerSecond": -1,
            "maxReadRequestsPerSecond": -1,
            "webhooks": [
                {
                    "url": "http://localhost:8000/api/pusher/webhook",
                    "event_types": [
                        "member_added",
                        "member_removed",
                        "channel_occupied",
                        "channel_vacated"
                    ]
                }
            ]
        }
    ]
}